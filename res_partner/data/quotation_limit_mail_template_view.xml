<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="0">
        <record id="quotation_limit_mail_template" model="mail.template">
            <field name="name">quotation limit mail template</field>
            <field name="model_id" ref="res_partner.model_res_partner"/>
            <field name="email_from">akash@gmail.com</field>
            <field name="email_to">shahil@gmail.com</field>
            <field name="subject">Quotation Limit</field>
            <field name="body_html" type="html">
                <div style="margin: 0px; padding: 0px; border:1px solid black;">
                    <p style="margin: 0px; padding: 0px; font-size: 13px;">
                        Hello ${object.email},
                        <br/>
                        Your credit/block limit has exceed for the ${object.name}.
                        <br/>
                        <ul>
                            <li>
                                <b>Customer Name -</b>
                                <span>${object.name}</span>
                            </li>
                            <li>
                                <b>Limit for Customer -</b>
                                <span>${object.name}</span>
                            </li>
                        </ul>
                        <br/>
                        Regards
                        <br/>
                        Aktiv Software
                    </p>
                </div>
            </field>
        </record>
    </data>

</odoo>